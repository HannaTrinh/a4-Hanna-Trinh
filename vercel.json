{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        },
        {
            "src": "package.json",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/scores",
            "dest": "/app.js"
        },
        {
            "src": "/config",
            "dest": "/app.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}